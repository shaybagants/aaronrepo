      </dependency>
      <dependency>
    <groupId>org.jfrog.test</groupId>
    <artifactId>multi1</artifactId>
    <version>5.3-SNAPSHOT</version>
</dependency>

  </dependencies>

<distributionManagement>
    <repository>
        <id>central</id>
        <name>Agile Markets Releases</name>
        <url>http://localhost:8081/artifactory/${artifactory.releases.repo.name}${artifactory.release.properties}</url>
