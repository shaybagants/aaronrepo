  <packaging>jar</packaging>
  <version>1.3-SNAPSHOT</version>
  <name>aaronapp</name>
  <url>http://maven.apache.org</url>
    <properties>

<artifactory.default.properties>;am.groupId=${project.groupId};am.artifactId=${project.artifactId};am.version=${project.version}</artifactory.default.properties>
<artifactory.snapshot.properties>${artifactory.default.properties}</artifactory.snapshot.properties>
